<launch>

  <arg name="launch_driver" default="true" />

  <group ns="imu">
    <group ns="gx5">
      <remap from="imu/data" to="imu_raw" />
      <include file="$(find microstrain_mips)/launch/microstrain.launch"  if="$(eval launch_driver==true)">
        <arg name="port" value="/dev/ttyACM0" />
        <arg name="imu_frame_id" value="imu" />
      </include>
    </group>

    <group ns="tamagawa">
      <node pkg="tamagawa_imu_driver" name="tag_serial_driver" type="tag_serial_driver" args="/dev/ttyUSB0 noGPS 100" if="$(eval launch_driver==true)">
        <remap from="/imu/data_raw" to="imu_raw" />
      </node>
    </group>

  </group>

</launch>
