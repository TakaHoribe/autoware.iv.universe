<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="lanelet_map_path" default="$(arg city_config_path)/kashiwanoha.osm"/>
  <arg name="pointcloud_map_path" default="$(arg city_config_path)/kashiwanoha.pcd"/>
  <arg name="map_tf_path" default="$(arg city_config_path)/kashiwanoha.launch"/>

  <include file="$(find autoware_launch)/launch/planning_simulator.launch" >
    <arg name="lanelet_map_path" value="$(arg lanelet_map_path )"/>
    <arg name="pointcloud_map_path" value="$(arg pointcloud_map_path)"/>
    <arg name="map_tf_path" value="$(arg map_tf_path)"/>
  </include>
</launch>
