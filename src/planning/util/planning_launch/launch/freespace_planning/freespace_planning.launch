<launch>
  <include file="$(find costmap_generator)/launch/costmap_generator.launch">
    <arg name="input_objects" value="/perception/prediction/objects"/>
    <arg name="input_points" value="/sensing/preprocess/lidar/no_ground/pointcloud"/>
    <arg name="input_lanelet_map_bin" value="/lanelet_map_bin"/>
    <arg name="output_costmap" value="~costmap"/>
    <arg name="output_occupancy_grid" value="~occupancy_grid"/>
  </include>

  <include file="$(find freespace_planner)/launch/astar_navi.launch">
    <arg name="input_route" value="/planning/mission_planning/route" />
    <arg name="input_occupancy_grid" value="costmap_generator/occupancy_grid" />
    <arg name="output_trajectory" value="~trajectory" />
  </include>
</launch>
