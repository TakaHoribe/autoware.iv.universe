<launch>

 <arg name="lanelet_map_path" default=""/>
 <arg name="points_map_path" default=""/>
 <arg name="map_tf_path" default=""/>

 <include file="$(find map_file)/launch/lanelet2_map_loader.launch">
   <arg name="file_name" default="$(arg lanelet_map_path)"/>
 </include>

 <node pkg="map_file" type="points_map_loader" name="points_map_loader" args="noupdate $(arg points_map_path)"/>

 <include file="$(arg map_tf_path)"/>

</launch>